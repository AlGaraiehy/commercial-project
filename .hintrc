{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "mask-clip",
          "accent-color"
        ]
      }
    ],
    "axe/forms": "off",
    "axe/parsing": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}